<%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %> <br> <br>

    <%= f.label :password %>
    <%= f.password_field :password %><br> <br>


    <%= f.label :remember_me do %>
        <%= f.check_box "remember_me"%> <br>
    <span>Remember me here</span>
    <% end %>
    <%= f.submit "Log in" %>
<% end %>
<p> Are you new here? <%= link_to "SignUp", signup_path %></p>



